  <logger name="HostLogger">
    <level value="ALL" />
    <appender-ref ref="HostAppender" />
    <!--<appender-ref ref="HostBlobAppender" />-->
  </logger>

  <logger name="NormalLogger">
    <level value="ALL" />
    <appender-ref ref="NormalAppender" />
    <!--<appender-ref ref="NormalBlobAppender" />-->
  </logger>
</log4net>
